{
    "name": "pforret/sunnysideup",
    "description": "Solar monitoring site data retrieval",
    "keywords": [
        "pforret",
        "php",
        "solar",
        "sunnysideup"
    ],
    "homepage": "https://github.com/pforret/sunnysideup",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\SunnySideUp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\SunnySideUp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.1"
}
